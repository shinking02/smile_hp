{
    "compilerOptions": {
        "strict": true,
        "sourceMap": true,
        "target": "ES5",
        "module": "ES2015",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "lib": [
            "ES2022",
            "dom"
        ],
        "esModuleInterop": true,
        "paths": {
            "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
        }
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "./src/**/*"
    ]
}